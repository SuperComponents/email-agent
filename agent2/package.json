{
  "name": "@proresponse/agent",
  "version": "0.1.0",
  "description": "Support agent worker thread for ProResponse AI",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "support",
    "agent",
    "worker",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@openai/agents": "^0.0.10",
    "dotenv": "^16.6.1",
    "openai": "^4.0.0",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/dotenv": "^8.0.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.6.1"
  },
  "files": [
    "dist"
  ]
}
