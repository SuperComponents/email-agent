{
  "name": "proresponse-agent",
  "version": "2.0.0",
  "description": "Enhanced TypeScript library providing an AI support agent assistant with email thread support, RAG integration, and automatic thread naming for support teams",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "node dist/index.js",
    "example": "node example.js",
    "example:enhanced": "node enhancedExample.js",
    "example:basic": "node example.js"
  },
  "keywords": [
    "ai",
    "support",
    "agent",
    "openai",
    "typescript",
    "customer-service",
    "email-threads",
    "rag",
    "retrieval-augmented-generation",
    "thread-naming",
    "sentiment-analysis",
    "escalation-detection",
    "chatgpt-style",
    "enhanced-agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "openai": "^4.67.3",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "@types/node": "^22.9.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-repo/proresponse-agent.git"
  },
  "bugs": {
    "url": "https://github.com/your-repo/proresponse-agent/issues"
  },
  "homepage": "https://github.com/your-repo/proresponse-agent#readme",
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "example.js",
    "enhancedExample.js"
  ],
  "features": {
    "emailThreadSupport": true,
    "ragIntegration": true,
    "threadNaming": true,
    "sentimentAnalysis": true,
    "escalationDetection": true,
    "multipleTools": true,
    "skeletonImplementation": true,
    "backwardCompatibility": true
  }
} 